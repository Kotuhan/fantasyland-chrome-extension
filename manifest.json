{
  "manifest_version": 2,

  "name": "Fantasyland chrome extension",
  "description": "This extension allaw to you play automaticly",
  "version": "1.0.2",

  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "src/popup.html",
    "default_title": "Click here!"
  },

  "background": {
    "scripts": ["src/random.js"],
    "persistent": false
  },

  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls> ",
    "http://*.fantasyland.ru/*"
  ]
}
